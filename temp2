Sub DeselectBlanksFromFilter()
    Dim ws As Worksheet
    Set ws = ThisWorkbook.Sheets("SD Report")
    
    With ws
        ' Check if filter is applied
        If .AutoFilterMode Then
            With .AutoFilter
                ' Check if filter exists in column F (6th column)
                If .Filters.Count >= 6 Then
                    ' Apply filter to exclude blanks
                    .Range.AutoFilter Field:=6, Criteria1:="<>"
                End If
            End With
        Else
           End If
    End With
End Sub
