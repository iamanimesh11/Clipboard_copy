def bubbble_sort(arr):
    n=len(arr)
    for i in range(n):
        print(f"ğŸ§  i is {i}-{arr[i]}")
        for j in range(0,n-i-1):
            print(f"ğŸ§© j is {j}-{arr[j]} and range is 0,{n-i-1}")
            print(arr)
            if arr[j]>arr[j+1]:
                arr[j],arr[j+1]=arr[j+1],arr[j]
                print(f"c: {arr}")
    return arr
    
print(bubbble_sort([5,1,4,2,8]))

