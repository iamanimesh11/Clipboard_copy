Sub FilterColumnGreaterThanZero()

    Dim ws As Worksheet
    Set ws = ActiveSheet   'or: Set ws = ThisWorkbook.Sheets("Sheet1")
    
    'Clear existing filters
    If ws.AutoFilterMode Then ws.AutoFilterMode = False
    
    'Apply filter on column C (change column number as needed)
    ws.Range("A1").CurrentRegion.AutoFilter _
        Field:=3, Operator:=xlGreater, Criteria1:="0"
        'Field:=3 means 3rd column (Column C)

End Sub