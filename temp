WITH user_profiles AS (
  SELECT 'u123' AS user_id, '{"name": "Animesh", "shipping_addresses": [{"city": "Delhi", "zip": "110001"}, {"city": "Mumbai", "zip": "400001"}]}' AS profile_json UNION ALL
  SELECT 'u456' AS user_id, '{"name": "Priya", "shipping_addresses": []}' AS profile_json, -- Empty array
  SELECT 'u789' AS user_id, '{"name": "Amit", "shipping_addresses": null}' AS profile_json -- NULL value
)