
  # array function:
select fullvisitorid,
       array(select distinct p.v2productName from UNNEST(hits) as h,UNNEST(h.product) as p where h.type ='EVENT') as product_names
      FROM `bigquery-public-data.google_analytics_sample.ga_sessions_20170801` 
      where fullvisitorid ='0750846065342433129'
       
