SELECT
  fullVisitorId,
 ( select count(*)
FROM   UNNEST(hits) AS h
WHERE  h.type = 'EVENT'
 ) as ev
from 
  `bigquery-public-data.google_analytics_sample.ga_sessions_20170801`
where fullVisitorId='3963931733144286855'

SELECT
fullvisitorid
from 
  `bigquery-public-data.google_analytics_sample.ga_sessions_20170801`
where fullVisitorId='3963931733144286855'
